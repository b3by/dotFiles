	email = b3by.in.th3.sky@gmail.com
	name = Antonio Bevilacqua

[alias]
  cm = commit -m
  a = add .
  p = push

[color]
  ui = true

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta
  plain = white
  old = red
  new = green
  whitespace = white reverse
  commit = yellow bold

[color "status"]
  added = yellow
  changed = green
  untracked = cyan
  branch = magenta
[push]
	default = simple
[credential]
	helper = osxkeychain
